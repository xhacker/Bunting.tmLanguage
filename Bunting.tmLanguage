	<key>patterns</key>
	<array>
		<dict>
			<key>comment</key>
			<string>Comment</string>
			<key>match</key>
			<string>//[^/\n]*(/[^/\n]+)*(//|/\n|\n|$)</string>
			<key>name</key>
			<string>comment.line.double-slash</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>Floating constant</string>
			<key>match</key>
			<string>(?&lt;=[^A-Za-z_~0-9])(\-?([0-9]+\.[0-9]+)(e(\+|\-)?[0-9]+)?)</string>
			<key>name</key>
			<string>constant.numeric</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>Integer constant</string>
			<key>match</key>
